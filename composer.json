{
    "name": "spurjobs/spectator",
    "description": "OpenAPI testing tools for your Laravel project",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "spectator", "openapi", "testing"],
    "require": {
        "php": "^7.2",
        "cebe/php-openapi": "^1.4",
        "illuminate/support": "^5.5|^6|^7",
        "justinrainbow/json-schema": "^5.2",
        "phpunit/phpunit": "^7|^8"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "authors": [
        {
            "name": "Adam Campbell",
            "email": "adam@spurjobs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Spectator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spectator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spectator\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
